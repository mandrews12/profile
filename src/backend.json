// schema for profile data updated with vs code
{
  "$schema": "http://json-schema.org/draft-04/schema",
  "type": "object",
  "patternProperties": {
    "^[a-zA-Z0-9]+$": {
      "type": "object",
      "properties": {
        "age": { "type": "string" },
        "country": { "type": "string" },
        "hobby": { "type": "string" },
        "image": { "type": "string", "format": "uri" },
        "name": { "type": "string" }
      },
      "required": ["age", "country", "hobby", "image", "name"]
    }
  }
}
